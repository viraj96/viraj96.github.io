{
	"manifest_version": 2,
	"name": "Zomby_Survival",
	"description": "Simple game using the power of p5js.",
	"version": "1.1",
	"browser_action": {
		"default_popup": "open.html"
	},
	"icons" : {
		"16" : "icon.png",
		"128" : "icon.png"
	},
	"background": {
		"scripts": ["libraries/p5.js", "libraries/p5.sound.js", "libraries/p5.dom.js", "sketch.js"],
		"persistent": false
	}
}